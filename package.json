    "name": "el-restoran",
    "scripts": {
        "watch:css": "nodemon -e scss -x \"npm run build:css\"",
        "build:css": "node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js --output-style compressed scss/estilos.scss css/main.css && npm run css-autoprefixer",
        "css-autoprefixer": "\"postcss -u autoprefixer -r css/main.css\""
    },
    "version": "1.0.2",
    "devDependencies": {
        "autoprefixer": "^9.7.1",
        "node-sass": "^4.14.1",
        "postcss-cli": "^6.1.3",
        "node-sass-glob-importer": "^5.3.2"
    },
    "main": "index.js",
    "dependencies": {
        "nodemon": "^2.0.6"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Diegolm357/proyecto-final-cuarta-entrega.git"
    },
    "bugs": {
